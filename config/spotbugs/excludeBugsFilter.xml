<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
  <Match>
    <Class name="~.*Test"/>
    <Bug pattern="UWF_FIELD_NOT_INITIALIZED_IN_CONSTRUCTOR" type="UWF_FIELD_NOT_INITIALIZED_IN_CONSTRUCTOR"/>
  </Match>
  <Match>
    <Class name="~.*Serialization*"/>
    <Bug pattern="SQL_NONCONSTANT_STRING_PASSED_TO_EXECUTE" type="SQL_NONCONSTANT_STRING_PASSED_TO_EXECUTE"/>
  </Match>
  <Match>
    <Class name="~.*"/>
    <Bug pattern="SQL_PREPARED_STATEMENT_GENERATED_FROM_NONCONSTANT_STRING" type="SQL_PREPARED_STATEMENT_GENERATED_FROM_NONCONSTANT_STRING"/>
  </Match>
  <Match>
    <Class name="~.*"/>
    <Bug pattern="EI_EXPOSE_REP" type="EI_EXPOSE_REP"/>
  </Match>
  <Match>
    <Class name="~.*"/>
    <Bug pattern="EI_EXPOSE_REP2" type="EI_EXPOSE_REP2"/>
  </Match>
</FindBugsFilter>